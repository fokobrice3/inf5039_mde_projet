<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated mymetamodel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="mymetamodel.presentation.MymetamodelModelWizardID"
            name="%_UI_MymetamodelModelWizard_label"
            class="mymetamodel.presentation.MymetamodelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/MymetamodelModelFile.gif">
         <description>%_UI_MymetamodelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated mymetamodel -->
      <editor
            id="mymetamodel.presentation.MymetamodelEditorID"
            name="%_UI_MymetamodelEditor_label"
            icon="icons/full/obj16/MymetamodelModelFile.gif"
            extensions="mymetamodel"
            class="mymetamodel.presentation.MymetamodelEditor"
            contributorClass="mymetamodel.presentation.MymetamodelActionBarContributor">
      </editor>
   </extension>

</plugin>
